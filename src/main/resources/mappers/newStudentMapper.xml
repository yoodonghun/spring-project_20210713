<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.example.lesson04.dao.NewStudentDAO">
    <insert id="insertNewStudent" parameterType="com.example.lesson04.model.NewStudent"
    useGeneratedKeys="true" keyProperty="id"> <!-- useGeneratedKeys: 생성된 키를 사용하겠다  -->
       INSERT INTO `new_student`
       (
          `name`
          ,`phoneNumber`
          ,`email`
          ,`dreamJob`
          ,`createdAt`
          ,`updatedAt`
       )
       VALUES
       (
          #{name}
          ,#{phoneNumber}
          ,#{email}
          ,#{dreamJob}
          ,NOW()
          ,NOW()
       )
    </insert>
    
    
 </mapper>